{
  "name": "@quercle/sdk",
  "version": "1.0.8",
  "description": "Official generated JavaScript/TypeScript SDK for Quercle API.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap --clean",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint ./src/index.ts ./src/quercle.ts ./src/tool-metadata.ts",
    "format": "biome format --write ./src/index.ts ./src/quercle.ts ./src/tool-metadata.ts",
    "knip": "knip --include dependencies,devDependencies",
    "test": "echo \"no tests for generated SDK\"",
    "check": "bun run typecheck && bun run lint && bun run knip",
    "prepublishOnly": "bun run check && bun run build"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quercledev/quercle-js.git"
  },
  "homepage": "https://quercle.dev",
  "author": "Quercle <support@quercle.dev>",
  "license": "MIT",
  "keywords": [
    "quercle",
    "api",
    "sdk",
    "typescript",
    "openapi"
  ],
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@types/node": "^22.18.6",
    "knip": "^5.66.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
